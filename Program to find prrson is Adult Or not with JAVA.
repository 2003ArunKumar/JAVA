Java code online
import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int age;
        System.out.println("Enter the Age:");
        age=sc.nextInt();
        if(age>=18){
            System.out.println("Adult.");
        }
        else{
            System.out.println("Not Adult.");
        }
    }
